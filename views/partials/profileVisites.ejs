<div class="clearfix" id="viewer">
	
	<h3>Les utilisateurs qui ont vu votre profile :</h3>
	<hr>

	<% if (listViewers) { %>
	 	<% for (user of listViewers) { %>
	 		<a class="viewer" href="/profile/<%= user.login %>">
				<div class="col-md-4">
					<h4><%= user.login %></h4>
					<img class="img-responsive" src="<% if (user.path) { %><%= user.path %><% } else { %>https://source.unsplash.com/random<% } %>">
					<p>
						<% if (user.age) { %>
							<%= user.age %> ans<br />
						<% } %>
						<%= user.viewed_at %>
					</p>
				</div>
			</a>
		<% } %>
	<% } else { %>
		<p>
			Personne n'a regard√© votre profile pour le moment.
		</p>
	<% } %>


</div>

